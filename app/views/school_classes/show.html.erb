<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @school_class.title %>
</p>
<h2>Students:</h2>
<ol>
<% @school_class.students.each do |student| %>
  <li><%= student.name %></li>
<% end %>
</ol>


<%= link_to 'Edit', edit_school_class_path(@school_class), class: "small secondary button" %> |
<%= link_to 'Back', school_classes_path, class: "small secondary button" %>
